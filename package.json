    "deploy": "./deploy.sh",
    "prepare": "patch-package"
  },
  "dependencies": {
    "bankai": "^9.0.0-1",
    "blob-to-buffer": "^1.2.6",
    "choo": "^6.0.0-4",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.2.1",
    "choo-service-worker": "^2.1.6",
    "dedent": "^0.7.0",
    "nanocomponent": "^6.0.0-1",
    "pokesav-ds-gen4": "file:../pokesav-ds-gen4",
    "pokesav-gba": "^1.0.1",
    "standard": "^10.0.2",
    "tachyons": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raymond-h/pokesav-showdown-converter-web"
  },
  "devDependencies": {
    "parcel-bundler": "^1.6.2",
    "patch-package": "^5.1.1"
  }

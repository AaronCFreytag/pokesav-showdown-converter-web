{
  "name": "pokesav-showdown-converter-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "parcel build --public-url / ./index.html",
    "start": "parcel -p 8080 ./index.html",
    "test": "standard && test-deps",
